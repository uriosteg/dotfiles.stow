# If not running interactively, don't do anything
[ -z "$PS1" ] && return

# Setup custom prompts
PS1='\[\e[1;38;5;111m\]\n┌[\d]-[\w]\n\[\e[38;5;77m\]└─[\!]-[\j]-[\u@\h]\$\[\e[0;38;5;171m\] '
PS2='>'
PS3='>>'
PS4='>>>'

# If environment file exists, load environment variables
if [ -f $DOTGENERAL/environment ]; then
   source $DOTGENERAL/environment
fi

# If aliases file exists, load aliases
if [ -f $HOME/.aliases ]; then
   source $HOME/.aliases
fi
